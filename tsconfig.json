{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@navigation": [
        "src/navigation"
      ],
      "@screens": [
        "src/screens"
      ],
      "@services": [
        "src/services"
      ],
      "@store": [
        "src/store"
      ],
      "@types": [
        "src/types"
      ],
      "@utils": [
        "src/utils"
      ]
    },
    "jsx": "react",
    "allowSyntheticDefaultImports": true
  }
}
